{
  "name": "express-lint",
  "version": "1.0.0",
  "description": "Express + Typescript + Lint",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "prettier:format": "prettier --config .prettierrc src/**/*.ts --write",
    "prettier:watch": "onchange src/**/*.ts -- prettier --write {{changed}}",
    "app:build": "rimraf ./dist && tsup",
    "dev:watch": "cross-env ENV_NAME=DEV nodemon",
    "dev:start": "cross-env ENV_NAME=DEV node dist/index.js"
  },
  "keywords": [],
  "author": "Sudhakar Jonnakuti",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.6.2",
    "http-terminator": "^3.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "envalid": "^7.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "_moduleAliases": {
    "@common": "dist/common",
    "@exception": "dist/exception",
    "@module": "dist/module",
    "@route": "dist/route"
  }
}